socket(AF_INET,SOCK_STREAM,IPPROTO_TCP)-->fd;
byte(16)-->p;
bzero(p,16);
w16(p,AF_INET);
w16(p#2,htons(12345));
bind(fd,p,16);
listen(fd,1);
0-->rfd;
while(accept(fd,0,0)->rfd>=0){
	byte(128*1024*1024)-->b;
	0-->sz;
	while(read3(rfd,b,128*1024*1024)->sz>0){
		write3(rfd,b,sz);
	};
	close(rfd);
};
close(fd);
